<div class="content-i">
    <div class="content-box">
        <div class="element-wrapper">
            <h6 class="element-header">List all users</h6>
            <div class="element-box">
                <div class="row">
                    <div class="col-md-12">
                        <h5 class="form-header">List all users</h5>
                    </div>
                </div>
                <hr>
                <div class="table-responsive">
                    <div class="row">
                        <div class="col-sm-12">
                            <table id="dataTable1" width="100%" class="table table-striped table-lightfont dataTable">
                                <thead>
                                    <tr>
                                        <th>name</th>
                                        <th>email</th>
                                        <th>primechain address</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {{#if users_list}}{{#each users_list}}
                                    <tr>
                                        <td>{{name}}</td>
                                        <td>{{email}}</td>
                                        <td>{{primechain_address}}</td>
                                    </tr>
                                    {{/each}}{{/if}}
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>